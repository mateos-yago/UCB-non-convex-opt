
% random initial conditions:

rng(138)

n = 4; % number of vectors
K = 16*(n^2); % number of arms
dim = 2*n;

Y0 = -6 + 12*rand(dim, K);  % (2n)xK initializations
    
d = 2;
f = randn(1, d+1);
g = randn(1, d+1);
mu = 1./(1:(2*d+1));

M = polyflow.mse_build_model(f, g, mu);



opts = struct();
opts.eta = 0.005;

loss_table = polyflow.mse_plot_ucb_trajectories_multiple_points(M, Y0, 1000, opts);